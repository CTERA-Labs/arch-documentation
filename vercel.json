{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/Insight-calculator",
      "destination": "/Insight-calculator/index.html"
    },
    {
      "source": "/Insight-calculator/",
      "destination": "/Insight-calculator/index.html"
    },
    {
      "source": "/Insight-presentation",
      "destination": "/Insight-presentation/index.html"
    },
    {
      "source": "/Insight-presentation/",
      "destination": "/Insight-presentation/index.html"
    },
    {
      "source": "/portal-modernization-presentation",
      "destination": "/portal-modernization-presentation/index.html"
    },
    {
      "source": "/portal-modernization-presentation/",
      "destination": "/portal-modernization-presentation/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

